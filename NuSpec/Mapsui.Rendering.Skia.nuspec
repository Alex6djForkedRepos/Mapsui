<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Mapsui</id>
    <version>$version$</version>
    <title>Mapsui</title>
    <authors>Paul den Dulk</authors>
    <owners>Paul den Dulk</owners>
    <projectUrl>https://github.com/Mapsui/Mapsui</projectUrl>
    <licenseUrl>https://github.com/Mapsui/Mapsui/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Mapsui - Library for mapping</description>
    <tags>map maps mapping geo gis osm</tags>
    <language>en-US</language>
    <dependencies>
      <!-- [ or ] is including, ( or ) is excluding -->
      <group targetFramework="netstandard2.0">
	    <dependency id="Mapsui" version="$version$"/>
	    <dependency id="Mapsui.Nts" version="$version$"/>
	    <dependency id="SkiaSharp" version="[2.88.2,3.0.0)"/>
        <dependency id="Svg.Skia" version="[0.5.12,0.6)"/>
        <dependency id="Topten.RichTextKit" version="[0.4.145,0.5)"/>
      </group>
      <group targetFramework="net6.0">
	      <dependency id="Mapsui" version="$version$"/>
	      <dependency id="Mapsui.Nts" version="$version$"/>
	      <dependency id="SkiaSharp" version="[2.88.2,3.0.0)"/>
	      <dependency id="Svg.Skia" version="[0.5.12,0.6)"/>
	      <dependency id="Topten.RichTextKit" version="[0.4.145,0.5)"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\LICENSE" target="LICENSE" />
    <file src="..\Mapsui.Rendering.Skia\bin\Release\netstandard2.0\Mapsui.Rendering.Skia.dll" target="lib\netstandard2.0\Mapsui.Rendering.Skia.dll" />
    <file src="..\Mapsui.Rendering.Skia\bin\Release\net6.0\Mapsui.Rendering.Skia.dll" target="lib\net6.0\Mapsui.Rendering.Skia.dll" />
  </files>
</package>
