      <group targetFramework="net6.0">
	      <dependency id="Mapsui" version="$version$"/>
	      <dependency id="BruTile" version="[5.0.0,6.0.0)"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\LICENSE" target="LICENSE" />
    <file src="..\Mapsui.Tiling\bin\Release\netstandard2.0\Mapsui.Tiling.dll" target="lib\netstandard2.0\Mapsui.Tiling.dll" />
    <file src="..\Mapsui.Tiling\bin\Release\net6.0\Mapsui.Tiling.dll" target="lib\net6.0\Mapsui.Tiling.dll" />
  </files>
</package>
